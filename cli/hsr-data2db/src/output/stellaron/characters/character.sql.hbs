("id", "character_id", "name", "max_level", "desc", "desc_params", "type", "element") VALUES
{{#each this.skills as |skill|}}
({{pg:escape skill.id}}, {{ character.id }}, {{pg:escape skill.name}}, {{ skill.max_level}}, {{pg:escape skill.desc}}, {{pg:array skill.params}}, {{pg:escape (stellaron:skillType skill.type)}}, {{pg:escape (stellaron:element skill.element)}})
{{/each}}

