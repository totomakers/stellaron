
INSERT INTO character
("id", "name", "element", "path", "rarity") VALUES
{{#each characters}}
({{pg:escape this.id}}, {{pg:escape this.name}},  {{pg:escape (stellaron:element this.element)}}, {{pg:escape (stellaron:path this.path)}}, {{this.rarity}});
{{/each}}

