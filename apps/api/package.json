{
  "name": "@stellaron/api",
  "private": true,
  "version": "1.0.0",
  "description": "Stellaron GraphQL API",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "prettier": "prettier \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs}/**/*.ts\" --fix",
    "drizzle:push": "drizzle-kit push:pg --config=src/drizzle-config.ts",
    "drizzle:studio": "drizzle-kit studio --config=src/drizzle-config.ts --port 3001 --verbose"
  },
  "dependencies": {
    "@apollo/server": "^4.9.1",
    "@neondatabase/serverless": "^0.5.7",
    "@nestjs/apollo": "^12.0.7",
    "@nestjs/common": "^10.1.3",
    "@nestjs/core": "^10.1.3",
    "@nestjs/devtools-integration": "^0.1.5",
    "@nestjs/graphql": "^12.0.8",
    "@nestjs/platform-express": "^10.1.3",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.19.12",
    "drizzle-orm": "^0.28.2",
    "graphql": "^16.8.0",
    "pg": "^8.11.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.11",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.1.3",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.76",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.5.0",
    "@types/pg": "^8.10.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  }
}
